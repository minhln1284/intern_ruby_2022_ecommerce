<tr>
  <td><%= category.name %></td>
  <td><%= find_parent category %></td>
  <td>
    <%= link_to icon("fa regular", "eye"), admin_category_path(category) %>
    <%= link_to icon("fa regular", "pen-to-square"), edit_admin_category_path(category), class: "td_custom" %>
    <% if can? :destroy, category %>
      <%= link_to icon("fa regular", "trash"), admin_category_path(category), method: :delete, data: { confirm: t("admin.category.you_sure") }%>
    <% end %>
  </td>
</tr>
